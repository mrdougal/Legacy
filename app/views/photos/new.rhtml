<% 
msg = 'Add a new photo'
title msg %>

<h1><%= msg %></h1>


<%= error_messages_for :photo %>

<% form_for(:photo, :url => photos_path, :html => {:multipart => true}) do |f| %>

<%= render :partial => 'form', :locals => {:f => f} %>

<p>
<%= submit_tag "Create new photo", {:class => 'send'} %>
</p>

<% end %>

